{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "chriskonnertz/string-calc": "^1.0",
        "econea/nusoap": "~0.9.5.1",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "~2.0",
        "intervention/image": "^2.4",
        "jaybizzle/crawler-detect": "1.*",
        "jenssegers/agent": "^2.6",
        "jimmyjs/laravel-report-generator": "^1.1",
        "kavenegar/laravel": "^1.1",
        "laravel-notification-channels/webpush": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "morilog/jalali": "2.*",
        "niklasravnsborg/laravel-pdf": "^3.1",
        "patrickschur/language-detection": "^3.4",
        "pragmarx/tracker": "^3.5",
        "spatie/laravel-permission": "^2.37",
        "spatie/laravel-sitemap": "^5.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
